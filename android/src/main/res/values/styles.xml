<resources xmlns:android="http://schemas.android.com/apk/res/android">
    
    <color name="aquadark">#358482</color>
    <color name="orange">#fa673d</color>
    <color name="drawer">#f0141414</color>

    <color name="swiper1">#fa673d</color>
    <color name="swiper2">#fa673d</color>
    <color name="swiper3">#D0B949</color>
    <color name="swiper4">#D0B949</color>

    <style name="AppTheme" parent="@android:style/Theme.Holo.Light.DarkActionBar">
        <item name="android:actionBarStyle">@style/BlackBar</item>
        <item name="android:progressBarStyleHorizontal">@style/ProgressBarRouteStory</item>
        <item name="android:seekBarStyle">@style/SeekBarRouteStory</item>
        <item name="android:editTextBackground">@drawable/routestory_edit_text_holo_light</item>
        <item name="android:autoCompleteTextViewStyle">@style/AutoCompleteTextViewRouteStory</item>
    </style>

    <style name="BlackBar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">#ff000000</item>
    </style>

    <style name="FullscreenDialog" parent="@android:style/Theme">
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowNoTitle">true</item>
    </style>

</resources>